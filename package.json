{
  "name": "graph-explorer",
  "displayName": "Graph Explorer",
  "description": "Graph Explorer plugin for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "microsoft-graph",
          "title": "Microsoft Graph",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "microsoft-graph": [
        {
          "id": "sample-queries",
          "name": "Sample Queries"
        },
        {
          "id": "history",
          "name": "History"
        }
      ]
    },
    "commands": [
      {
        "command": "sampleQuery.Click",
        "title": "click"
	  },
	  {
        "command": "history.populate",
        "title": "populate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.0-next.0"
  }
}
